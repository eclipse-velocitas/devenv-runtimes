{
    "container_name": "seatservice",
    "image": {
        "name": "ghcr.io/eclipse/kuksa.val.services/seat_service:v0.2.0"
    },
    "host_config": {
        "devices": [],
        "network_mode": "host",
        "privileged": false,
        "restart_policy": {
            "maximum_retry_count": 0,
            "retry_timeout": 0,
            "type": "unless-stopped"
        },
        "runtime": "io.containerd.runc.v2",
        "extra_hosts": [],
        "port_mappings": [],
        "log_config": {
            "driver_config": {
                "type": "json-file",
                "max_files": 2,
                "max_size": "1M",
                "root_dir": ""
            },
            "mode_config": {
                "mode": "blocking",
                "max_buffer_size": ""
            }
        }
    },
    "config": {
        "env": [
            "BROKER_ADDR=127.0.0.1:55555",
            "RUST_LOG=info",
            "vehicle_data_broker=info"
        ],
        "cmd": []
    }
}
